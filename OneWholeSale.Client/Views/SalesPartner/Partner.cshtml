@model OneWholeSale.Model.Dto.Partner.PartnerDto
@{
    ViewData["Title"] = "Partner";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="subheader">
    <h1 class="subheader-title">
        <i class="subheader-icon fal fa-edit"></i> Create Partner
    </h1>
</div>
<div class="row">
    <div class="col-xl-12 sortable-grid ui-sortable">
        <div id="panel-1" class="panel panel-sortable" role="widget">
            <div class="panel-hdr" role="heading">
                <h2 class="ui-sortable-handle">
                    Partner
